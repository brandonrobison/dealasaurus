<!DOCTYPE html>
<html>
	<head>
		<title>Dealasaur.us</title>
		<%= favicon_link_tag 'favicon.ico' %>
		<%= stylesheet_link_tag "application", media: "all" %>
		<%= javascript_include_tag "application" %>
		<%= csrf_meta_tags %>
		<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
		<![endif]-->
	</head>
	<body>
		<%= render 'layouts/navigation' %>
		<div class="container">
			<%= render 'layouts/messages' %>
			<%= yield %>

			<p  style="font-size:11px; color: white"><br/><br/><br/><br/><br/><br/><br/><br/>For Brandon's debug purposes:<br/>
			controller: <%= params[:controller] %><br/>
			action: <%= params[:action] %><br/></p>
		</div>
	</body>
</html>



